{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      1,
      {
        "singleQuote": true,
        "printWidth": 80
      }
    ],
    "no-console": 0,
    "import/prefer-default-export": 0
  }
}
